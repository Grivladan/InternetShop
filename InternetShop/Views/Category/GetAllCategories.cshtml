@model IEnumerable<DataAccess.Entities.Category>

@{
    Layout = null;
}

<div id="categories-list-name">Categories</div> 
<div id="categories-list">
    @foreach(var item in Model)
    {
        <p>@Html.ActionLink(@item.Name, "GetProductsByCategory", "Product", new { categoryId = item.Id}, null)</p>
    }
</div>

